# Network Configuration
NETWORK=devnet  # or mainnet
RPC_ENDPOINT=https://api.devnet.solana.com
WS_ENDPOINT=wss://api.devnet.solana.com

# Wallet Configuration
PRIVATE_KEY=your_base58_encoded_private_key_here

# Executor Configuration
USE_JITO=true
USE_BLOXROUTE=true
USE_NOZOMI=true

# Jito Configuration
JITO_TIP_AMOUNT=0.0001

# BloxRoute Configuration
BLOXROUTE_AUTH_HEADER=your_bloxroute_auth_header
BLOXROUTE_ENDPOINT=https://virginia.solana.dex.blxrbdn.com
BLOXROUTE_FEE=0.001

# Nozomi Configuration
NOZOMI_API_KEY=your_nozomi_api_key
NOZOMI_ENDPOINT=https://ams1.secure.nozomi.temporal.xyz/?c=
NOZOMI_TIP_AMOUNT=0.0001

# Performance Configuration
MAX_RETRY_ATTEMPTS=3
CONFIRMATION_TIMEOUT=30000
TRANSACTION_TIMEOUT=10000

# Logging Configuration
LOG_LEVEL=info
ENABLE_PERFORMANCE_LOGGING=true
